{
  "compilerOptions": {
    "module": "nodenext",
    "moduleResolution": "nodenext",
    "target": "es2022",
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"]
    },
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "strict": false,
    "strictNullChecks": false,
    "strictFunctionTypes": false
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
